{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "buildCommand": "npm run vercel-build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "functions": {
      "*.js": {
        "maxDuration": 30,
        "memory": 1024
      }
    }
  }